include(BundleUtilities)

set(TARGET_FILE_PATH "@TARGET_FILE_PATH@")
set(CMAKE_BUILD_TYPE "@CMAKE_BUILD_TYPE@")
set(RUNTIME_DIRS @RUNTIME_DIRS@)
set(RUNTIME @RUNTIME@)
set(RUNTIME_DEBUG @RUNTIME_DEBUG@)
set(RUNTIME_RELEASE @RUNTIME_RELEASE@)
set(ACMAKE_FIXUP_COMMON_PATH @ACMAKE_FIXUP_COMMON_PATH@)
set(CONFIGURATIONS @CONFIGURATIONS@)

if(CONFIGURATIONS)
    list(FIND CONFIGURATIONS "${CMAKE_BUILD_TYPE}" CONTAIN_BUILD_TYPE)
    if(CONTAIN_BUILD_TYPE EQUAL -1)
        return()
    endif()
endif()

include(${ACMAKE_FIXUP_COMMON_PATH})
